+	background: transparent;
+}
+a:active, a:hover {
+	outline: 0;
+}
+
+/* Text-level semantics
+	 ========================================================================== */
+abbr {
+	letter-spacing: 0.1em;
+}
+abbr[title] {
+	border-bottom: 1px dotted;
+}
+b, strong {
+	font-weight: bold;
+}
+dfn {
+	font-style: italic;
+}
+mark {
+	background: #ff0;
+	color: #000;
+}
+small {
+	font-size: 70%;
