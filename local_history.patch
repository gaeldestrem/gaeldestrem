+	display: inline-block;	/* 1 */
+	vertical-align: baseline;	/* 2 */
+}
+audio:not([controls]) {
+	display: none;
+	height: 0;
+}
+[hidden], template {
+	display: none;
+}
+
+/* Links
+	 ========================================================================== */
